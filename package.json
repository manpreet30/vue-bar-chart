{
  "name": "@manpreet30/vue-bar-graph",
  "version": "1.3.0",
  "description": "Simple and lightweight Vue chart svg component without chart library dependencies",
  "author": {
    "name": "Manpreet Singh",
    "email": "manpreettech30@gmail.com"
  },
  "repository": "https://github.com/manpreet30/vue-bar-graph",
  "bugs": "https://github.com/manpreet30/vue-bar-graph/issues",
  "keywords": [
    "vue",
    "chart",
    "bar"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-bar-graph.common.js",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-bar-graph src/wrapper.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "gsap": "^3.6.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@vue/airbnb"
    ],
    "rules": {
      "linebreak-style": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
